<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".homepage.activitys.ArtSetThreeActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white"
        android:fitsSystemWindows="true"
        >
        <com.hjq.bar.TitleBar
            android:id="@+id/title_bar"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@color/background_art_set"
            app:title="@string/art_set"
            app:titleColor="#000000"
            app:leftTitle="返回首页"
            app:rightTitle="分享"
            app:backButton="false"
            app:titleSize="17sp"
            app:lineColor="@color/background_art_set"
            app:rightColor="@color/Tv_text"
            />
    </LinearLayout>
</com.zhy.autolayout.AutoLinearLayout>